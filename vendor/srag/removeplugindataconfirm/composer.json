{
  "name": "srag/removeplugindataconfirm",
  "description": "Demand if plugin data should be removed on uninstall",
  "version": "0.8.1",
  "extra": {
    "ilias_plugin": {
      "ilias_min_version": "6.0",
      "ilias_max_version": "7.999"
    }
  },
  "type": "library",
  "keywords": [
    "confirm",
    "data",
    "ilias",
    "plugin",
    "uninstall"
  ],
  "homepage": "https://github.com/fluxapps/RemovePluginDataConfirm",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "srag/dic": ">=0.1.0",
    "srag/librarylanguageinstaller": ">=0.1.0"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\RemovePluginDataConfirm\\DhbwTraining\\": "src/"
    },
    "classmap": [
      "src/class.RemovePluginDataConfirmCtrl.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "update-version": [
      "srag\\GeneratePluginInfosHelper\\UpdateVersion::updateVersion"
    ]
  }
}
