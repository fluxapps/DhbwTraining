{
  "name": "srag/dic",
  "description": "Use all ILIAS globals in your class",
  "version": "0.31.3",
  "extra": {
    "ilias_plugin": {
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999"
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_LIBRARY"
  },
  "type": "library",
  "keywords": [
    "ILIAS",
    "DIC",
    "Trait",
    "globals"
  ],
  "homepage": "https://github.com/studer-raimann/DIC",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_LDIC"
  },
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\DIC\\DhbwTraining\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "generate-plugin-readme": [
      "srag\\GeneratePluginInfosHelper\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}